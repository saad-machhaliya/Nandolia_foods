    <form action="" method="POST">
      <table class="table table-success table-striped" style="width: 1124px;">
         <thead>
           <tr>
             <th scope="col">id</th>
             <th scope="col">Name</th>
             <th scope="col">price</th>
             <th scope="col">category id</th>
             <th scope="col">image</th>
             <th scope="col">Action</th>
           </tr>
         
         <% if(usersData.length > 0){ %>
     
           <% for(var i=0;i<usersData.length; i++){ %>
               <tr>
                   <td>  <%- usersData[i].item_id %>  </td>
                   <td>  <%- usersData[i].item_name %>  </td>
                   <td>  <%- usersData[i].item_price %>  </td>
                   <td>  <%- usersData[i].cat_id %>  </td>
                   <td>  <img src="<%= "../images/" + usersData[i].item_image %>" width="100px" height="100px"  alt="errorsss" >  </td>
                   <td>  <a class="btn btn-danger" href="products/delete_products?item_id=<%- usersData[i].item_id %>" role="button">DELETE</a> &nbsp &nbsp
                      <a class="btn btn-secondary" href="products/update_products?item_id=<%- usersData[i].item_id %>" role="button">UPDATE</a></td>
               </tr>
           <% } %>
     
         <% } else{ %>
          
           <h1>students data not found</h1>
         <% } %>
     
       </table>
     </form>
    

